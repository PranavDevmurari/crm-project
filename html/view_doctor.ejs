<%- include('header')%>

          <!-- Content wrapper -->
          <div class="content-wrapper">
            <!-- Content -->

            <div class="container-xxl flex-grow-1 container-p-y">
              

              <!-- Basic Bootstrap Table -->
              <div class="card">
                <h5 class="card-header">active data</h5>
                <div class="table-responsive text-nowrap">
                  <table class="table">
                    <thead>
                      <tr>
                        <th>no</th>
                        <th>hospital name</th>
                        <th>doctor name</th>
                        <th>doctor qualification</th>
                        <th>doctor salary</th>
                        <th>Status</th>                      
                      </tr>
                    </thead>
                    <tbody class="table-border-bottom-0">
                      <% var no = '1'; for(var d of activedata) {%>                    
                        <tr>
                          <th scope="row"><%= no ++ %></th>
                          <td><%= d.hospitalid.hname %></td>
                          <td><%= d.name%></td>                                              
                          <td><%= d.qualification%></td>                                              
                          <td><%= d.salary%></td>                                              
                          <td><a href="/doctor/deactive/<%=d.id%>">deactive</a></td>                
                         
                        </tr>
                        <% } %>
                    </tbody>
                  </table>
                </div>
              </div>
              <div class="card mt-5">
                <h5 class="card-header">deactive data</h5>
                <div class="table-responsive text-nowrap">
                  <table class="table">
                    <thead>
                      <tr>
                        <th>no</th>
                        <th>hospital name</th>
                        <th>doctor name</th>
                        <th>doctor qualification</th>
                        <th>doctor salary</th>
                        <th>Status</th>                    
                      </tr>
                    </thead>
                    <tbody class="table-border-bottom-0">
                      <% var no = '1'; for(var d of deactivedata) {%>                    
                        <tr>
                          <th scope="row"><%= no ++ %></th>
                          <td><%= d.hospitalid.hname %></td>
                          <td><%= d.name%></td>                                              
                          <td><%= d.qualification%></td>                                              
                          <td><%= d.salary%></td>                                              
                          <td><a href="/doctor/active/<%=d.id%>">active</a></td>                
                         
                        </tr>
                        <% } %>
                    </tbody>
                  </table>
                </div>
              </div>
              <!--/ Basic Bootstrap Table -->

          </div> 
    <!-- Core JS -->
    <!-- build:js assets/vendor/js/core.js -->
    <%- include('footer')%>
  </body>
</html>
